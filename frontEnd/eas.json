{
  "cli": {
    "requireCommit": true,
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "distribution": "internal",
      "channel": "production",
      "npm": "install"
    }
  },
  "submit": {
    "production": {}
  }
}